  <!-- Document icon -->
  <rect x="150" y="80" width="200" height="240" fill="white" stroke="#4CAF50" stroke-width="3" rx="5"/>
  
  <!-- Document lines -->
  <line x1="180" y1="120" x2="320" y2="120" stroke="#4CAF50" stroke-width="2" opacity="0.5"/>
  <line x1="180" y1="150" x2="320" y2="150" stroke="#4CAF50" stroke-width="2" opacity="0.5"/>
  <line x1="180" y1="180" x2="320" y2="180" stroke="#4CAF50" stroke-width="2" opacity="0.5"/>
  
  <!-- Checkmark -->
